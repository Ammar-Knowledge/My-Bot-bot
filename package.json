		"index.js",
		"README.md"
	],
	"dependencies": {
		"@commitlint/config-conventional": "^17.3.0",
		"@commitlint/core": "^17.3.0",
		"js-yaml": "^4.1.0",
		"lint": "^0.8.8",
		"probot": "^12.2.8",
		"semantic-release": "^19.0.5"
	},
	"devDependencies": {
		"jest": "^29.3.1",
		"smee-client": "^1.2.3"
	},
	"engines": {
		"node": "10.x",
		"npm": "5.5.x"
	},
	"jest": {
		"testEnvironment": "node"
	}
}
